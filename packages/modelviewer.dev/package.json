    "@types/prismjs": "^1.9.0",
    "@types/resize-observer-browser": "^0.1.2",
    "@webcomponents/webcomponentsjs": "~2.1.3",
    "focus-visible": "^5.0.1",
    "intersection-observer": "^0.11.0",
    "lit-element": "^2.4.0",
