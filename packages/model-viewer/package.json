{
  "name": "@google/model-viewer",
  "version": "1.1.0",
  "description": "Easily display interactive 3D models on the web and in AR!",
  "repository": "https://github.com/GoogleWebComponents/model-viewer",
  "bugs": {
    "url": "https://github.com/GoogleWebComponents/model-viewer/issues"
  },
  "homepage": "https://github.com/GoogleWebComponents/model-viewer#readme",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "contributors": [
    "Jordan Santell <jsantell@google.com>",
    "Chris Joel <cdata@google.com>",
    "Emmett Lalish <elalish@google.com>",
    "Ricardo Cabello <ricardocabello@google.com>",
    "Matt Small <mbsmall@google.com>",
    "Yuin Chien <yuin@google.com>"
  ],
  "main": "dist/model-viewer-umd.js",
  "module": "dist/model-viewer.min.js",
  "files": [
    "dist/model-viewer.js",
    "dist/model-viewer.js.map",
    "dist/model-viewer.min.js",
    "dist/model-viewer.min.js.map",
    "dist/model-viewer-umd.js",
    "dist/model-viewer-umd.js.map",
    "dist/model-viewer-legacy.js",
    "POLYFILLS.md"
  ],
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "prepare": "if [ ! -L './shared-assets' ]; then ln -s ./node_modules/@google/model-viewer-shared-assets ./shared-assets; fi",
    "build": "npm run build:tsc && npm run build:rollup",
    "build:dev": "npm run build:tsc && npm run build:rollup:dev",
    "build:tsc": "tsc --incremental",
    "build:rollup": "rollup -c --environment NODE_ENV:production",
    "build:rollup:dev": "rollup -c --environment NODE_ENV:development",
    "create-legacy-bundles": "node ./scripts/create-legacy-bundles.js",
    "prepublishOnly": "npm run build && npm run create-legacy-bundles",
    "test": "karma start --single-run",
    "test:ci": "npm run test",
    "test:ci:legacy": "karma start ./karma-legacy.conf.js --single-run",
    "check-fidelity": "node ./test/fidelity/index.js ./test/fidelity/config.json",
    "compare-fidelity": "./scripts/compare-fidelity-to-ref.sh",
    "update:package-lock": "rm ./package-lock.json && npm i --only=production",
    "serve": "node_modules/.bin/http-server -a 127.0.0.1 -c-1",
    "dev": "npm run build:dev && npm-run-all --parallel 'watch:tsc -- --preserveWatchOutput' 'watch:test' 'serve -- -s'",
    "watch:tsc": "tsc -w --incremental",
    "watch:rollup": "rollup -c -w --environment NODE_ENV:production",
    "watch:rollup:dev": "rollup -c -w --environment NODE_ENV:development",
    "watch:test": "karma start",
    "watch:test:legacy": "karma start ./karma-legacy.conf.js"
  },
  "keywords": [
    "ar",
    "gltf",
    "glb",
    "webar",
    "webvr",
    "webxr",
    "arcore",
    "arkit",
    "webaronarcore",
    "webaronarkit",
    "augmented reality",
    "model-viewer",
    "3d"
  ],
  "devDependencies": {
    "@google/3dom": "0.1.0",
    "lit-element": "^2.2.0",
    "three": "^0.119.0",
    "@types/resize-observer-browser": "^0.1.2",
    "@google/model-viewer-shared-assets": "^0.0.1",
    "@jsantell/event-target": "^1.1.2",
    "@open-wc/karma-esm": "^2.11.1",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.5",
    "@types/pngjs": "^3.4.0",
    "@ungap/event-target": "^0.1.0",
    "@webcomponents/webcomponentsjs": "~2.1.3",
    "chai": "^4.1.2",
    "focus-visible": "^5.0.1",
    "http-server": "^0.12.1",
    "intersection-observer": "^0.5.1",
    "karma": "^5.0.2",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^2.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "lit-html": "^1.1.2",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "object.values": "^1.1.1",
    "polymer-build": "^3.1.1",
    "resize-observer-polyfill": "^1.5.0",
    "rollup": "^2.23.1",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-polyfill": "^3.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "3.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}